{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "/schema/message/vote.json",
    "type": "object",
    "properties": {
        "hash": {
            "type": "string",
            "pattern": "^[A-Za-z0-9_-]{43}$"
        },
        "votes": {
            "$ref": "../tx/v1/votes.json"
        }
    },
    "required": [
        "hash",
        "votes"
    ],
    "additionalProperties": false
}
