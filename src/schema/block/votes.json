{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "/schema/block/votes.json",

  "type": "object",
  "properties": {
    "orgn": {
      "type": "string",
      "pattern": "^[A-Za-z0-9_-]{43}$"
    },
    "sig": {
      "type": "string",
      "pattern": "^[A-Za-z0-9_-]{86}$"
    }
  },
  "additionalProperties": false,
  "required": [
    "orgn",
    "sig"
  ]
}